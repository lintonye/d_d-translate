{
  "name": "D_D Translate",
  "action": {},
  "manifest_version": 3,
  "version": "0.1",
  "description": "Translate the current page",

  "background": {
    "service_worker": "background.ts"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-scripts/main.tsx"],
      "css": ["content-scripts/main.css"]
    }
  ]
}
